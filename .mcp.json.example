{
  "mcpServers": {
    "wix": {
      "command": "npx",
      "args": ["-y", "@wix/mcp-remote@latest", "https://mcp.wix.com/mcp"]
    },
    "discord-events": {
      "command": "node",
      "args": ["mcp-servers/discord-events-mcp-server/dist/index.js"],
      "env": {
        "DISCORD_BOT_TOKEN": "",
        "DISCORD_GUILD_ID": ""
      }
    },
    "google-calendar": {
      "command": "node",
      "args": ["mcp-servers/google-calendar-mcp-server/dist/index.js"],
      "env": {
        "GOOGLE_SERVICE_ACCOUNT_KEY_PATH": "",
        "GOOGLE_CALENDAR_ID": ""
      }
    },
    "mailchimp": {
      "command": "node",
      "args": ["mcp-servers/mailchimp/dist/index.js"],
      "env": {
        "MAILCHIMP_API_KEY": ""
      }
    }
  }
}
